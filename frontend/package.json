  "scripts": {
    "start": "react-scripts start",
    "start:dev": "react-scripts start",
    "start:backend": "cd ../backend && python -m uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "start:worker": "cd ../backend && python -m celery -A app.services.worker.celery_app worker --loglevel=info --pool=solo",
    "start:all": "echo 'Please run the following commands in separate terminals:' && echo '1. npm run start:backend' && echo '2. npm run start:worker' && echo '3. npm run start:dev'",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
